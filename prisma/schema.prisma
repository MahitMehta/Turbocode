generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  email   String   @unique
  pass    String   
  username    String? 
  projectIds String[] @db.ObjectId 
}

model UserFile {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  name    String
  code    String   @default("")   
  filePath String
  srcProjectId String @db.ObjectId
}

model UserProject {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  ownerId String   @db.ObjectId 
  name String  
}
